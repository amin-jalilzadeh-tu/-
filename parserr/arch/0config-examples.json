  "full_analysis_config": {
    "main_config": {
      "parsing": {
        "perform_parsing": true,
        "parse_mode": "selective",
        "building_selection": {
          "mode": "range",
          "building_range": {"start": 0, "end": 10}
        },
        "idf_content": {
          "mode": "selective",
          "exclude_categories": ["schedules"],
          "parse_relationships": true
        },
        "sql_content": {
          "mode": "selective",
          "variables": {
            "mode": "categories",
            "categories": ["energy", "comfort", "hvac"]
          },
          "time_filter": {
            "months": [1, 2, 6, 7, 8, 12]
          },
          "frequency_filter": ["Hourly", "Daily"],
          "zone_filter": {
            "mode": "pattern",
            "zone_patterns": ["*ZONE*"],
            "exclude_zones": ["PLENUM"]
          }
        },
        "output_options": {
          "save_format": "parquet",
          "compression": "snappy",
          "create_summary": true,
          "save_metadata": true
        },
        "performance": {
          "batch_size": 100,
          "parallel_parsing": false,
          "memory_limit_mb": 2048,
          "chunk_large_files": true
        },
        "validation": {
          "validate_before_parsing": true,
          "continue_on_error": true,
          "save_validation_report": true
        }
      }
    }
