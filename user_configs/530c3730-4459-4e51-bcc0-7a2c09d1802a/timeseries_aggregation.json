{
  "timeseries_aggregation": {
    "default_settings": {
      "target_frequencies": [
        "monthly",
        "yearly"
      ],
      "skip_existing": false
    },
    "input_options": {
      "use_base_data": true,
      "use_variant_data": true
    },
    "aggregation_rules": {
      "default_method": "sum",
      "by_pattern": [
        {
          "pattern": [
            "energy",
            "electricity",
            "heating",
            "cooling"
          ],
          "method": "sum"
        },
        {
          "pattern": [
            "temperature",
            "humidity"
          ],
          "method": "mean"
        },
        {
          "pattern": [
            "rate",
            "power"
          ],
          "method": "mean"
        }
      ]
    }
  }
}